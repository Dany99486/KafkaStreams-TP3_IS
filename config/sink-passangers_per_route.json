{
  "name": "jdbc-sink-results",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
    "tasks.max": "2",
    "connection.url": "jdbc:postgresql://database:5432/project3",
    "connection.user": "postgres",
    "connection.password": "My01pass",
    "topics": "projeto3_passengers_per_route,projeto3_available_seats_per_route,projeto3_occupancy_per_route,projeto3_total_passengers",
    "table.name.format": "${topic}",
    "auto.create": "true",
    "auto.evolve": "true",
    "insert.mode": "upsert",
    "pk.mode": "record_value",
    "pk.fields": "routeId",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "true",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter"
  }
}